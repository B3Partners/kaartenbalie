<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
        <class name="nl.b3p.kaartenbalie.core.server.reporting.domain.tables.RowValue" table="rep_rowvalue">
        <id name="id" type="int" column="rva_id" >
            <generator class="native"/>
        </id>
        <many-to-one name="tableRow" column="rva_tro_id" update="true" not-null="false"/>  
        <property name="rowValue" column="rva_rowValue"/>
        <property name="valueOrder" column="rva_valueOrder"/>
    </class>
</hibernate-mapping>