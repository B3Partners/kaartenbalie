<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="nl.b3p.ogc.wfs.v110.WfsServiceProvider" table="Wfs_ServiceProvider">
        <id name="id" type="int" column="WFSSERVICEPROVIDERID">
            <generator class="native"/>
        </id>
        
        
        <set name="wfsLayers" inverse="true" cascade="all" lazy="true">
            <key column="WFSSERVICEPROVIDERID"/>
            <one-to-many class="nl.b3p.ogc.wfs.v110.WfsLayer"/>
        </set>
        
        
        <property name="name">
            <column name="NAME" default="0" not-null="true" length="60"/>
        </property>
        <property name="abbr">
            <column name="ABBR" default="0" not-null="true" length="60"/>
        </property>
        <property name="title">
            <column name="TITLE" default="0" not-null="true" length="50"/>
        </property>
        <property name="givenName">
            <column name="GIVENNAME" not-null="true" default="0" length="50"/>
        </property>
        <property name="url">
            <column name="URL" default="0" not-null="true"  length="4000"/>
        </property>
        <property name="updatedDate">
            <column name="UPDATEDDATE" not-null="true" />
        </property>
        <property name="wfsVersion">
            <column name="WFSVERSION" not-null="true" default="0" length="50"/>
        </property>
    </class>
</hibernate-mapping>