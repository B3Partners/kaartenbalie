<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="nl.b3p.wms.capabilities.ServiceDomainResource" table="ServiceDomainResource">
        <id name="id" type="int" column="SDRID">
            <generator class="native"/>
        </id>
        
        <many-to-one name="serviceProvider" column="SERVICEPROVIDERID" not-null="true"/>
       
        <set name="formats" lazy="false" table="ServiceDomainFormat">
            <key column="SDRID" not-null="true"/>
            <element column="format" type="string" not-null="true" length="100"/>
        </set>
        
        <property name="domain">
            <column name="DOMAIN" not-null="true"  default="0" length="50"/>
        </property>
        <property name="getUrl">
            <column name="GETURL" default="0" length="4000"/>
        </property>
        <property name="postUrl">
            <column name="POSTURL" length="4000"/>
        </property>
    </class>
</hibernate-mapping>