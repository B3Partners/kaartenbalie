<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="nl.b3p.wms.capabilities.Identifier" table="Identifier">
    	<id name="id" type="int" column="IDENTIFIERID" >
            <generator class="native"/>
        </id>
        
        <many-to-one name="layer" column="LAYERID" not-null="true"/>
        
        <property name="authorityName">
            <column name="AUTHORITYNAME"/>
        </property>
        <property name="authorityURL">
            <column name="AUTHORITYURL"/>
        </property>
    </class>
</hibernate-mapping>