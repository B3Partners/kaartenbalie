<hibernate-mapping>
  <class discriminator-value="ReportTemplate" name="nl.b3p.kaartenbalie.core.server.reporting.domain.BaseReport" table="rep_report">
    <id column="rep_id" name="id" type="int">
      <generator class="native"/>
    </id>
    <discriminator column="rep_discriminator" type="string"/>
    <many-to-one column="rpd_org_id" name="owningOrganization" not-null="false"/>
    <set cascade="all, delete-orphan" inverse="true" name="dataTables">
      <key column="tab_rep_id"/>
      <one-to-many class="nl.b3p.kaartenbalie.core.server.reporting.domain.tables.DataTable"/>
    </set>
    <property column="rep_reportDate" name="reportDate"/>
    <property column="rep_processingTime" name="processingTime"/>
    <subclass discriminator-value="DataUsageReport" name="nl.b3p.kaartenbalie.core.server.reporting.datausagereport.DataUsageReport">
      <set cascade="all, delete-orphan" inverse="true" name="reportData">
        <key column="rpd_rep_id"/>
        <one-to-many class="nl.b3p.kaartenbalie.core.server.reporting.datausagereport.RepData"/>
      </set>
      <set lazy="true" name="userIds" table="rep_users">
        <key column="rpd_usr_id"/>
        <element column="usr_id" type="int"/>
      </set>
      <property column="rep_startDate" name="startDate"/>
      <property column="rep_endDate" name="endDate"/>
      <property column="rep_organizationId" name="organizationId"/>
    </subclass>
  </class>
</hibernate-mapping>
