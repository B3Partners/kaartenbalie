<hibernate-mapping>
  <class discriminator-value="ReportData" name="nl.b3p.kaartenbalie.core.server.reporting.datausagereport.RepData" table="rep_reportdata">
    <id column="rpd_id" name="id" type="int">
      <generator class="native"/>
    </id>
    <discriminator column="rpd_discriminator" type="string"/>
    <many-to-one column="rpd_rep_id" name="report"/>
    <subclass discriminator-value="ReportDataDetails" name="nl.b3p.kaartenbalie.core.server.reporting.datausagereport.RepDataDetails">
      <property column="rpd_minHour" name="minHour"/>
      <property column="rpd_maxHour" name="maxHour"/>
      <set cascade="all, delete-orphan" inverse="true" name="usageDetails">
        <key column="usd_rpd_id"/>
        <one-to-many class="nl.b3p.kaartenbalie.core.server.reporting.datausagereport.UsageDetails"/>
      </set>
    </subclass>
    <subclass discriminator-value="ReportDataSummary" name="nl.b3p.kaartenbalie.core.server.reporting.datausagereport.RepDataSummary">
      <property column="rpd_ctoHits" name="ctoHits"/>
      <property column="rpd_ctoAverageResponse" name="ctoAverageResponse"/>
      <property column="rpd_cioHits" name="cioHits"/>
      <property column="rpd_cioAverageResponse" name="cioAverageResponse"/>
      <property column="rpd_cxoHits" name="cxoHits"/>
      <property column="rpd_cxoData" name="cxoData"/>
      <property column="rpd_cxoAverageResponse" name="cxoAverageResponse"/>
      <property column="rpd_roHits" name="roHits"/>
      <property column="rpd_roUpload" name="roUpload"/>
      <property column="rpd_roDownload" name="roDownload"/>
      <property column="rpd_roAverageResponse" name="roAverageResponse"/>
    </subclass>
  </class>
</hibernate-mapping>
