<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="nl.b3p.wms.capabilities.ContactInformation" table="ContactInformation">
        <id name="id" type="int" column="SERVICEPROVIDERID">
            <generator class="foreign">
                <param name="property">serviceProvider</param>
            </generator>
        </id>
        
        <one-to-one name="serviceProvider" constrained="true" class="nl.b3p.wms.capabilities.ServiceProvider" />
        
        <property name="contactPerson">
            <column name="CONTACTPERSON" length="50"/>
        </property>
        <property name="contactPosition">
            <column name="CONTACTPOSITION" length="50"/>
        </property>
        <property name="address">
            <column name="ADDRESS" length="50"/>
        </property>
        <property name="addressType">
            <column name="ADDRESSTYPE" length="50"/>
        </property>
        <property name="postcode">
            <column name="POSTCODE" length="50"/>
        </property>
        <property name="city">
            <column name="CITY" length="50"/>
        </property>
        <property name="stateOrProvince">
            <column name="STATEORPROVINCE" length="50"/>
        </property>
        <property name="country">
            <column name="COUNTRY" length="50"/>
        </property>
        <property name="voiceTelephone">
            <column name="VOICETELEPHONE" length="50"/>
        </property>
        <property name="fascimileTelephone">
            <column name="FASCIMILETELEPHONE" length="50"/>
        </property>
        <property name="emailAddress">
            <column name="EMAILADDRESS" length="50"/>
        </property>
    </class>
</hibernate-mapping>