<hibernate-mapping>
  <class name="nl.b3p.wms.capabilities.ContactInformation" table="ContactInformation">
    <id column="SERVICEPROVIDERID" name="id" type="int">
      <generator class="foreign">
        <param name="property">serviceProvider</param>
      </generator>
    </id>
    <one-to-one class="nl.b3p.wms.capabilities.ServiceProvider" constrained="true" name="serviceProvider"/>
    <property name="contactPerson">
      <column length="50" name="CONTACTPERSON"/>
    </property>
    <property name="contactPosition">
      <column length="50" name="CONTACTPOSITION"/>
    </property>
    <property name="address">
      <column length="50" name="ADDRESS"/>
    </property>
    <property name="addressType">
      <column length="50" name="ADDRESSTYPE"/>
    </property>
    <property name="postcode">
      <column length="50" name="POSTCODE"/>
    </property>
    <property name="city">
      <column length="50" name="CITY"/>
    </property>
    <property name="stateOrProvince">
      <column length="50" name="STATEORPROVINCE"/>
    </property>
    <property name="country">
      <column length="50" name="COUNTRY"/>
    </property>
    <property name="voiceTelephone">
      <column length="50" name="VOICETELEPHONE"/>
    </property>
    <property name="fascimileTelephone">
      <column length="50" name="FASCIMILETELEPHONE"/>
    </property>
    <property name="emailAddress">
      <column length="50" name="EMAILADDRESS"/>
    </property>
  </class>
</hibernate-mapping>
