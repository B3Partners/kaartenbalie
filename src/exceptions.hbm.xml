<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="nl.b3p.kaartenbalie.core.server.Exceptions" table="Exceptions">
    	<id name="id" type="int" column="EXCEPTIONSID">
            <generator class="native"/>
        </id>
        
        <many-to-one name="serviceProvider" column="SERVICEPROVIDERID" not-null="true" insert="false" update="false"/>
        
        <set name="formats" lazy="true">
            <key column="EXCEPTIONSID"/>
            <element column="FORMAT" type="string" not-null="true"/>
        </set>
    </class>
</hibernate-mapping>