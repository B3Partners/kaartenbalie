<hibernate-mapping>
  <class name="nl.b3p.wms.capabilities.ServiceProvider" table="ServiceProvider">
    <id column="SERVICEPROVIDERID" name="id" type="int">
      <generator class="native"/>
    </id>
    <one-to-one cascade="all" class="nl.b3p.wms.capabilities.ContactInformation" name="contactInformation"/>
    <set cascade="all" inverse="true" lazy="true" name="layers">
      <key column="SERVICEPROVIDERID"/>
      <one-to-many class="nl.b3p.wms.capabilities.Layer"/>
    </set>
    <set cascade="all" lazy="true" name="serviceProviderKeywordList" table="serviceproviderkeywordlist">
      <key column="SERVICEPROVIDERID"/>
      <element column="KEYWORD" length="50" not-null="true" type="string"/>
    </set>
    <set cascade="all" inverse="true" lazy="true" name="domainResource" table="serviceDomainResource">
      <key column="SERVICEPROVIDERID"/>
      <one-to-many class="nl.b3p.wms.capabilities.ServiceDomainResource"/>
    </set>
    <set cascade="all" lazy="true" name="exceptions" table="exceptions">
      <key column="SERVICEPROVIDERID"/>
      <element column="FORMAT" length="50" not-null="true" type="string"/>
    </set>
    <property name="name">
      <column default="0" length="60" name="NAME" not-null="true"/>
    </property>
    <property name="abbr">
      <column default="0" length="60" name="ABBR" not-null="true"/>
    </property>
    <property name="title">
      <column default="0" length="255" name="TITLE" not-null="true"/>
    </property>
    <property name="abstracts" type="text">
      <column length="65530" name="ABSTRACTS"/>
    </property>
    <property name="fees" type="text">
      <column length="65530" name="FEES"/>
    </property>
    <property name="accessConstraints" type="text">
      <column length="65530" name="ACCESSCONSTRAINTS"/>
    </property>
    <property name="givenName">
      <column default="0" length="50" name="GIVENNAME" not-null="true"/>
    </property>
    <property name="url">
      <column default="0" length="4000" name="URL" not-null="true"/>
    </property>
    <property name="updatedDate">
      <column name="UPDATEDDATE" not-null="true"/>
    </property>
    <property name="wmsVersion">
      <column default="0" length="50" name="WMSVERSION" not-null="true"/>
    </property>
  </class>
</hibernate-mapping>
