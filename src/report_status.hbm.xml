<hibernate-mapping>
  <class name="nl.b3p.kaartenbalie.core.server.reporting.domain.ThreadReportStatus" table="rep_status">
    <id column="sta_id" name="id" type="int">
      <generator class="native"/>
    </id>
    <many-to-one column="sta_org_id" name="organization" not-null="false" update="true"/>
    <property column="sta_reportId" name="reportId"/>
    <property column="sta_creationDate" name="creationDate"/>
    <property column="sta_state" name="state"/>
    <property column="sta_statusMessage" length="4000" name="statusMessage"/>
  </class>
</hibernate-mapping>
