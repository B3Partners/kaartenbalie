<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="nl.b3p.wms.capabilities.Dimensions" table="Dimensions">
    	<id name="id" type="int" column="DIMENSIONSID" >
            <generator class="native"/>
        </id>
        
        <many-to-one name="layer" column="LAYERID" not-null="true"/>
        
        <property name="dimensionsName">
            <column name="DIMENSIONSNAME" length="50"/>
        </property>
        <property name="dimensionsUnit">
            <column name="DIMENSIONSUNIT" length="50"/>
        </property>
        <property name="dimensionsUnitSymbol">
            <column name="DIMENSIONSUNITSYMBOL" length="50"/>
        </property>
        <property name="extentName">
            <column name="EXTENTNAME" length="50"/>
        </property>
        <property name="extentDefaults">
            <column name="EXTENTDEFAULTS" length="50"/>
        </property>
        <property name="extentNearestValue">
            <column name="EXTENTNEARESTVALUE" length="50"/>
        </property>
        <property name="extentMultipleValues">
            <column name="EXTENTMULTIPLEVALUES" length="50"/>
        </property>
        <property name="extentCurrent">
            <column name="EXTENTCURRENT" length="50"/>
        </property>
    </class>
</hibernate-mapping>