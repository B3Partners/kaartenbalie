<hibernate-mapping>
  <class name="nl.b3p.wms.capabilities.Dimensions" table="Dimensions">
    <id column="DIMENSIONSID" name="id" type="int">
      <generator class="native"/>
    </id>
    <many-to-one column="LAYERID" name="layer" not-null="true"/>
    <property name="dimensionsName">
      <column length="50" name="DIMENSIONSNAME"/>
    </property>
    <property name="dimensionsUnit">
      <column length="50" name="DIMENSIONSUNIT"/>
    </property>
    <property name="dimensionsUnitSymbol">
      <column length="50" name="DIMENSIONSUNITSYMBOL"/>
    </property>
    <property name="extentName">
      <column length="50" name="EXTENTNAME"/>
    </property>
    <property name="extentDefaults">
      <column length="50" name="EXTENTDEFAULTS"/>
    </property>
    <property name="extentNearestValue">
      <column length="50" name="EXTENTNEARESTVALUE"/>
    </property>
    <property name="extentMultipleValues">
      <column length="50" name="EXTENTMULTIPLEVALUES"/>
    </property>
    <property name="extentCurrent">
      <column length="50" name="EXTENTCURRENT"/>
    </property>
  </class>
</hibernate-mapping>
