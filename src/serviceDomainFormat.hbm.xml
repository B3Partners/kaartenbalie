<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="nl.b3p.kaartenbalie.core.server.ServiceDomainFormat" table="ServiceDomainFormat">
        <id name="id" type="int" column="FORMATID">
            <generator class="native"/>
        </id>
        
        <many-to-one name="serviceProvider" column="SERVICEPROVIDERID" not-null="true" insert="false" update="false"/>
       
        <property name="domain">
            <column name="DOMAIN"/>
        </property>        
        <property name="format">
            <column name="FORMAT"/>
        </property>
    </class>
</hibernate-mapping>