<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="nl.b3p.wms.capabilities.StyleDomainResource" table="StyleDomainResource">
        <id name="id" type="int" column="SDRID" >
            <generator class="native"/>
        </id>
        
        <many-to-one name="style" column="STYLEID" not-null="true"/>
        
        <set name="formats" lazy="true" table="StyleDomainFormat">
            <key column="SDRID" not-null="true"/>
            <element column="format" type="string" not-null="true" length="45"/>
        </set>

        <property name="domain">
            <column name="DOMAIN" length="45" not-null="true"/>
        </property>
        <property name="url">
            <column name="URL" length="4000" not-null="true"/>
        </property>
        <property name="width">
            <column name="WIDTH" length="45"/>
        </property>
        <property name="height">
            <column name="HEIGHT" length="45"/>
        </property>
    </class>
</hibernate-mapping>