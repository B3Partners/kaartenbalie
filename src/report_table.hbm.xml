<hibernate-mapping>
  <class name="nl.b3p.kaartenbalie.core.server.reporting.domain.tables.DataTable" table="rep_table">
    <id column="tab_id" name="id" type="int">
      <generator class="native"/>
    </id>
    <many-to-one column="tab_rep_id" name="baseReport"/>
    <set cascade="all, delete-orphan" inverse="true" name="tableRows" order-by="tro_rowOrder ASC">
      <key column="tro_tab_id"/>
      <one-to-many class="nl.b3p.kaartenbalie.core.server.reporting.domain.tables.TableRow"/>
    </set>
    <property column="rep_tableName" name="tableName"/>
  </class>
</hibernate-mapping>
