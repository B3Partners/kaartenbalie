<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
        <class name="nl.b3p.kaartenbalie.core.server.reporting.domain.tables.DataTable" table="rep_table">
        <id name="id" type="int" column="tab_id" >
            <generator class="native"/>
        </id>
        <many-to-one name="baseReport" column="tab_rep_id"/>  
        <set name="tableRows" inverse="true"  cascade="all, delete-orphan" order-by="tro_rowOrder ASC">
            <key column="tro_tab_id"/>
            <one-to-many class="nl.b3p.kaartenbalie.core.server.reporting.domain.tables.TableRow"/>
        </set>   

        <property name="tableName" column="rep_tableName"/>
    </class>
</hibernate-mapping>