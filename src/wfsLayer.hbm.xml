<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="nl.b3p.ogc.wfs.v110.WfsLayer" table="wfs_layer">
    	<id name="id" type="int" length="10">
            <!-- sql-type="INTEGER UNSIGNED"-->
            <column name="WFSLAYERID"/>
            <generator class="native" />
        </id>

        <many-to-one name="wfsServiceProvider" column="WFSSERVICEPROVIDERID" not-null="true"/><!--   /-->
                
        <property name="name">
            <column name="NAME" length="200"/>
        </property>
        <property name="title">
            <column name="TITLE"  not-null="true" length="255" default="0"/>
        </property>
        <property name="metaData" type="text">
            <column name="METADATA" length="65530"/>
        </property>
    </class>
</hibernate-mapping>