<hibernate-mapping>
  <class name="nl.b3p.ogc.wfs.v110.WfsLayer" table="wfs_layer">
    <id length="10" name="id" type="int">
      <!-- sql-type="INTEGER UNSIGNED"-->
      <column name="WFSLAYERID"/>
      <generator class="native"/>
    </id>
    <many-to-one column="WFSSERVICEPROVIDERID" name="wfsServiceProvider" not-null="true"/>
    <!--   /-->
    <property name="name">
      <column length="200" name="NAME"/>
    </property>
    <property name="title">
      <column default="0" length="255" name="TITLE" not-null="true"/>
    </property>
    <property name="metaData" type="text">
      <column length="65530" name="METADATA"/>
    </property>
  </class>
</hibernate-mapping>
