<hibernate-mapping>
  <class discriminator-value="ServerProviderRequest" name="nl.b3p.kaartenbalie.core.server.reporting.domain.requests.ServiceProviderRequest" table="mon_serviceproviderrequest">
    <id column="spr_id" name="id" type="int">
      <generator class="native"/>
    </id>
    <discriminator column="spr_discriminator" type="string"/>
    <many-to-one column="spr_clr_id" name="clientRequest" not-null="false" update="true"/>
    <property column="spr_bytesSend" name="bytesSend"/>
    <property column="spr_bytesReceived" name="bytesReceived"/>
    <property column="spr_responseStatus" name="responseStatus"/>
    <property column="spr_providerRequestURI" length="4000" name="providerRequestURI"/>
    <property column="spr_requestResponseTime" name="requestResponseTime"/>
    <property column="spr_msSinceRequestStart" name="msSinceRequestStart"/>
    <property column="spr_exceptionClass" name="exceptionClass"/>
    <property column="spr_exceptionMessage" length="4000" name="exceptionMessage"/>
    <subclass discriminator-value="WMSRequest" name="nl.b3p.kaartenbalie.core.server.reporting.domain.requests.WMSRequest">
      <property column="spr_wmsVersion" name="wmsVersion"/>
      <property column="spr_serviceProviderId" name="serviceProviderId"/>
      <subclass discriminator-value="WMSGetMapRequest" name="nl.b3p.kaartenbalie.core.server.reporting.domain.requests.WMSGetMapRequest">
        <property column="spr_srs" name="srs"/>
        <property column="spr_width" name="width"/>
        <property column="spr_height" name="height"/>
        <property column="spr_format" name="format"/>
      </subclass>
      <subclass discriminator-value="WMSGetFeatureInfoRequest" name="nl.b3p.kaartenbalie.core.server.reporting.domain.requests.WMSGetFeatureInfoRequest">
            </subclass>
      <subclass discriminator-value="WMSGetLegendGraphicRequest" name="nl.b3p.kaartenbalie.core.server.reporting.domain.requests.WMSGetLegendGraphicRequest">
            </subclass>
      <subclass discriminator-value="WMSGetCapabilitiesRequest" name="nl.b3p.kaartenbalie.core.server.reporting.domain.requests.WMSGetCapabilitiesRequest">
            </subclass>
    </subclass>
  </class>
</hibernate-mapping>
