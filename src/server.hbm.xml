<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="nl.b3p.kaartenbalie.core.Server" table="Server">
    	<id name="id" type="long" column="SERVERID" >
            <generator class="assigned"/>
        </id>
        <property name="serverServ">
            <column name="SERVICEPROVIDERID" />
        </property>
        <property name="serverCapa">
            <column name="CAPABILITYID"/>
        </property>
        <property name="serverName">
            <column name="NAME"/>
        </property>
        <property name="serverUrl">
            <column name="URL"/>
        </property>
        <property name="serverUpdatedDate">
            <column name="UPDATEDATE"/>
        </property>
        <property name="serverReviewed">
            <column name="REVIEWED"/>
        </property>
    </class>
</hibernate-mapping>