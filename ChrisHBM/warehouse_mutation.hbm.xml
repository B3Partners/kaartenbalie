<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="nl.b3p.kaartenbalie.core.server.datawarehousing.domain.EntityMutation" table="wah_mutation">
        
        <id name="id" type="int" column="mut_id" >
            <generator class="native"/>
        </id>
        <many-to-one name="warehousedEntity" column="mut_wae_id" update="true" not-null="false"/>        
        <set name="propertyValues" > 
            <key column="val_mut_id" update="true" not-null="false" /> 
            <one-to-many class="nl.b3p.kaartenbalie.core.server.datawarehousing.domain.PropertyValue"/> 
        </set>          
        <property name="mutationDate" column="mut_mutationDate"/>      
        
        

</class>
</hibernate-mapping>