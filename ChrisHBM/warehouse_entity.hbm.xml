<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="nl.b3p.kaartenbalie.core.server.datawarehousing.domain.WarehousedEntity" table="wah_entity">
        
        <id name="id" type="int" column="wae_id" >
            <generator class="native"/>
        </id>
        <set name="entityDataMappings" > 
            <key column="edm_wae_id" update="true" not-null="false" /> 
            <one-to-many class="nl.b3p.kaartenbalie.core.server.datawarehousing.domain.DataMappingTemplate"/> 
        </set>
        
        <property name="objectClass" column="wae_objectClass" index="wae_objectClass_index"/>
        <property name="referencedId" column="wae_referencedId" index="wae_referencedId_index"/>
    </class>
</hibernate-mapping>