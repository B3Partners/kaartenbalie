<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="nl.b3p.kaartenbalie.core.server.datawarehousing.domain.WarehousedEntity" table="wah_entity">
        
        <id name="id" type="int" column="wae_id" >
            <generator class="native"/>
        </id>
        <set name="entityMutations" > 
            <key column="mut_wae_id" update="true" not-null="false" /> 
            <one-to-many class="nl.b3p.kaartenbalie.core.server.datawarehousing.domain.EntityMutation"/> 
        </set>
        <many-to-one name="entityClass" column="wae_cls_id" update="true" not-null="false"/>       
        <property name="referencedId" column="wae_referencedId" index="wae_referencedId_index"/>
        <property name="dateCreated" column="wae_dateCreated"/>
        <property name="dateDeleted" column="wae_dateDeleted"/>        

</class>
</hibernate-mapping>