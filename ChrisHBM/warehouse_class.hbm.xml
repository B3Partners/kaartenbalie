<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="nl.b3p.kaartenbalie.core.server.datawarehousing.domain.EntityClass" table="wah_class">
        
        <id name="id" type="int" column="cls_id" >
            <generator class="native"/>
        </id>
        <set name="entityProperties" > 
            <key column="prp_cls_id" update="true" not-null="false" /> 
            <one-to-many class="nl.b3p.kaartenbalie.core.server.datawarehousing.domain.EntityProperty"/> 
        </set>
        <set name="warehousedEntities" > 
            <key column="wae_cls_id" update="true" not-null="false" /> 
            <one-to-many class="nl.b3p.kaartenbalie.core.server.datawarehousing.domain.WarehousedEntity"/> 
        </set>
        
        <property name="objectClass" column="wae_objectClass" index="wae_objectClass_index"/>
        
    </class>
</hibernate-mapping>