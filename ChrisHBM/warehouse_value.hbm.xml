<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="nl.b3p.kaartenbalie.core.server.datawarehousing.domain.PropertyValue" table="wah_value">
        
        <id name="id" type="int" column="val_id" >
            <generator class="native"/>
        </id>
        <many-to-one name="entityProperty" column="val_prp_id" update="true" not-null="false"/>        
        <many-to-one name="entityMutation" column="val_mut_id" update="true" not-null="false"/>                
        <property name="objectData" column="val_objectData"/>      
    </class>
</hibernate-mapping>